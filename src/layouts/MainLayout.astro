---

interface Props {
	title: string;
}
const { title } = Astro.props;
import Header from '../components/Required/Header.astro';
import Menus from '../components/Required/menus/Menus.astro';
import Footer from '../components/Required/Footer.astro';
---

<!-- Default layout for all webpages -->

<!doctype html>
<html>
    <head>
        <meta charset="UTF-8" />
		<meta name="description" content=" The portfolio of Cyborgnetical." />
		<meta content="width=device-width, initial-scale=1" name="viewport" />
		<link rel="icon" type="image/svg+xml" href="/public/NJROTC-Seal.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>Parlier NJROTC | {title}</title>
    </head>
    <body style="margin:0px; p">
        <Header></Header>
        <Menus></Menus>
        <slot />
        <Footer></Footer>

    </body>
</html>