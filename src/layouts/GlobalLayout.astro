---

interface Props {
	title: string;
    frontmatter?
}
const { frontmatter , title = frontmatter.title  } = Astro.props;

import Header from '../components/Required/Header.astro';
import Menus from '../components/Required/menus/Menus.astro';
import Footer from '../components/Required/Footer.astro';
---

<!-- Default layout for all .md files -->

<!doctype html>
<html>
    <head>
        <meta charset="UTF-8" />
		<meta name="description" content=" The portfolio of Cyborgnetical." />
		<meta content="width=device-width, initial-scale=1" name="viewport" />
		<link rel="icon" type="image/svg+xml" href="/NJROTC-Seal.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>Parlier NJROTC | {title}</title>
    </head>
    <body style="margin:0px; p">
        <Header></Header>
        <Menus></Menus>
        <slot name="maintitle"/>
        <slot />
        <Footer></Footer>
        <style is:global>
            body *{
                color: white;
                font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
            }
            .page-title{
                display: none;
            }
            h1,h2,h3,h4,h5,h6{
                font-weight: normal;
            }
            h1{
                font-size: 2.5em;
                font-family: "Josefin sans";
                font-weight: bold;
            }
            h2{
                font-size: 2em;
            }
            @font-face {
                font-family: "Josefin Sans"; /** Look its me */
                src: url("/fonts/JosefinSans-VariableFont_wght.ttf");
            }
        </style>
        
    </body>
</html>