---

interface Props {
	title: string;
}
const { title } = Astro.props;
import Header from '../components/Header.astro';
---

<!-- Default layout for all webpages -->

<!doctype html>
<html>
    <head>
        <meta charset="UTF-8" />
		<meta name="description" content=" The portfolio of Cyborgnetical." />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/public/NJROTC-Seal.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>Parlier NJROTC | {title}</title>
    </head>
    <body style="margin:0px; p">
        <Header client:load></Header>
        <slot />
    </body>
</html>